<AnchorPane prefHeight="400.0" prefWidth="280.0" style="-fx-background-color: #2a2f33;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.front.Dark.DaddMarkStudent">
